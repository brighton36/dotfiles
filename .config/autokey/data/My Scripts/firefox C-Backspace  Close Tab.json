{
    "type": "script",
    "description": "firefox \"C-Backspace\" : Close Tab",
    "store": {},
    "modes": [],
    "usageCount": 0,
    "prompt": false,
    "omitTrigger": false,
    "showInTrayMenu": false,
    "abbreviation": {
        "abbreviations": [],
        "backspace": true,
        "ignoreCase": false,
        "immediate": false,
        "triggerInside": false,
        "wordChars": "[\\w]"
    },
    "hotkey": {
        "modifiers": [
            "<ctrl>"
        ],
        "hotKey": "<backspace>"
    },
    "filter": {
        "regex": "Navigator.firefox",
        "isRecursive": false
    }
}